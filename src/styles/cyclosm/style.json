{
  "version": 8,
  "sources": {
    "ne2_shaded": {
      "maxzoom": 20,
      "tileSize": 256,
      "tiles": [
        "https://a.tile-cyclosm.openstreetmap.fr/cyclosm/{z}/{x}/{y}.png",
        "https://b.tile-cyclosm.openstreetmap.fr/cyclosm/{z}/{x}/{y}.png",
        "https://c.tile-cyclosm.openstreetmap.fr/cyclosm/{z}/{x}/{y}.png"
      ],
      "type": "raster"
    },
    "openmaptiles": {
      "type": "vector",
      "url": "https://tiles.openfreemap.org/planet"
    }
  },
  "sprite": "https://tiles.openfreemap.org/sprites/ofm_f384/ofm",
  "glyphs": "https://tiles.openfreemap.org/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#eee8d5"
      }
    },
            {
    "id": "thunderforest-background",
    "type": "raster",
    "source": "ne2_shaded",
    "minzoom": 0,
    "maxzoom": 22
    },
        {
      "id": "amenities",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "poi",
      "minzoom": 12,
      "filter": [
        "all",
        [
          "==",
          "$type",
          "Point"
        ],
        [
          "in",
          "subclass",
          "hospital",
          "pharmacy"
        ]
      ],
      "layout": {
        "icon-image": [
          "get",
          "subclass"
        ],
        "icon-size": 1,
        "icon-allow-overlap": true
      },
      "paint": {
        "icon-opacity": 0.5
      }
    },
        {
      "id": "amenities_important",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "poi",
      "minzoom": 12,
      "filter": [
        "all",
        [
          "==",
          "$type",
          "Point"
        ],
        [
          "in",
          "subclass",
          "bicycle",
          "toilets",
          "drinking_water"
        ]
      ],
      "layout": {
        "icon-image": [
          "get",
          "subclass"
        ],
        "icon-size": 1,
        "icon-allow-overlap": true
      },
      "paint": {
        "icon-opacity": 1
      }
    }
  ],
  "id": "basic"
}